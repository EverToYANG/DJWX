#banner_box ul li p {
	position: absolute;
	z-index: 2;
	display: block;
	width: 100%;
	bottom: 0;
	background-color: rgba(0, 0, 0, 0.5);
	color: #F4F4F4;
	font-size: 14px;
	text-indent: 55px;
	line-height: 24px;
	text-align: left;
	text-indent: 10px;
	text-overflow: ellipsis;
	white-space: nowrap;
	padding: 0;
	margin: 0;
}

