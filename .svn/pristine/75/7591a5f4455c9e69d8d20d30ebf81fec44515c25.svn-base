/*-------------------------·ÖÒ³*/
.pages{
	margin:auto 0px;
	text-align:left;
}

.pages_list{
	margin-left: auto;
	margin-right: auto; 
	line-height:40px;
	font-size:1.5rem;
	clear:both;
	display:block; width:100%;
	zoom:1
}
.pages_list:after{content:".";display:block;height:0;clear:both;visibility:hidden}
.pages_list strong{
	color:#FFF;
	padding-left:10px;
	padding-right:10px;
	background:#F60;
	border:1px solid #C33;
	margin: 0px 10px 10px 0px;
	display:block; float:left;
}
.pages_list a{
	height:40px;
	padding-left:10px;
	padding-right:10px;
	border:1px solid #DDD;
	background:#FFF;
	text-decoration:none;
	margin-right:5px;
	color:#000;
	margin-bottom:10px; display:block; float:left;
}
.pages_list a:hover{
	color:#FF0000;
}

