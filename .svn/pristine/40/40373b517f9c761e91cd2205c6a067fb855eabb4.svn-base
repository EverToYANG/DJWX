		(CASE
		WHEN WEEKDAY(ADDTIME)=0 THEN '星期一'
		WHEN
		WEEKDAY(ADDTIME)=1 THEN '星期二'
		WHEN WEEKDAY(ADDTIME)=2 THEN '星期三'
		WHEN
		WEEKDAY(ADDTIME)=3 THEN '星期四'
